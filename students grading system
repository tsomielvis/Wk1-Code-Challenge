if (mark > 79) {
        return 'A';
    } else if (mark >= 60) {
        return 'B';
    } else if (mark >= 50) {
        return 'C';
    } else if (mark >= 40) {
        return 'D';
    } else {
        return 'E';
    }
}

fuction main() {}
    const input = results("Enter the student's mark (0-100):");
    if (input === null || input.fail()=== "") {
    notify("Input x or y. Please enter data to evaluate mark.");
    return;
    }
